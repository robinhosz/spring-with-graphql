

type Cliente {
    id:ID!
    nome:String
    email:String
    createdAt: Timestamp!
}

input ClienteInput {
    id:ID
    nome:String
    email:String
}

extend type Query {
    getClienteById(id:ID!):Cliente

    findAllClientes:[Cliente]!

}

extend type Mutation {
    saveCliente(cliente:ClienteInput):Cliente!
    deleteCliente(id:ID!):Boolean
}